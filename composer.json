{
  "name" : "ipub/websockets-wamp",
  "type" : "library",
  "description" : "Extension for implementing WAMP protocol into ipub/websockets server",
  "keywords" : [
    "nette",
    "websockets",
    "wamp",
    "ipub",
    "ipublikuj",
    "framework",
    "tools"
  ],

  "homepage" : "https://github.com/iPublikuj/websockets-wamp",

  "license" : [
    "GPL-2.0",
    "GPL-3.0"
  ],

  "authors" : [
    {
      "name" : "iPublikuj:cms",
      "email" : "info@ipublikuj.eu",
      "homepage" : "https://www.ipublikuj.eu/"
    }
  ],

  "support" : {
    "email" : "support@ipublikuj.eu",
    "issues" : "https://github.com/iPublikuj/websockets-wamp/issues"
  },

  "extra" : {
    "ipub" : {
      "configuration" : {
        "extension" : "IPub\\WebSocketsWAMP\\DI\\WebSocketsWAMPExtension"
      }
    }
  },

  "require" : {
    "php": ">=7.4.0|>=8.0.0",
    "nette/di": "^3.0",
    "nette/utils": "^3.2",
    "psr/log": "^1.1|^3.0",
    "symfony/serializer" : "^3.0|^4.0|^5.0|^6.0",
    "ipub/websockets" : "^1.5"
  },

  "require-dev" : {
    "mockery/mockery": "^1.4",
    "nette/bootstrap": "^3.1",
    "nette/mail": "^3.1",
    "nette/robot-loader": "^3.4",
    "nette/safe-stream": "^2.5",
    "nette/security": "^3.1",
    "nette/tester": "^2.4",
    "ninjify/nunjuck": "^0.3",
    "ninjify/qa": "^0.12",
    "pds/skeleton": "^1.0",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-doctrine": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "symfony/console": "^5.0|^6.0",
    "symfony/event-dispatcher": "^5.0|^6.0",
    "tracy/tracy": "^2.9"
  },

  "autoload" : {
    "psr-4" : {
      "IPub\\WebSocketsWAMP\\" : "src/"
    }
  },

  "autoload-dev" : {
    "psr-4" : {
      "Tests\\Cases\\" : "tests/cases",
      "Tests\\Fixtures\\" : "tests/fixtures",
      "Tests\\Libs\\" : "tests/libs"
    }
  },

  "config" : {
    "sort-packages" : true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "phpstan/extension-installer": true
    }
  }
}
